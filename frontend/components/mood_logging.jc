# mood_logging.jc
# Mood Logging Component

function render_mood_logging() {
    print("=== Mood Logging ===")
    
    # Example: Select mood (for demo, we hardcode a mood)
    mood_selection = "Happy"

    # Call Jac backend walker to log mood
    result = Spawn("log_mood", mood_selection)
    print("Mood logged:", result)
}
